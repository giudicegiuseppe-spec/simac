<script>
                                $("#invia-notifica-aggiornamento-preventivo").live("click", function(){
                                    if( !confirm("Sei sicuro di voler inviare la notifica?") )
                                        return false;

                                    $(this).css("visibility", "hidden");
                                    $("#overlay").show();
                                    var formData = new FormData();
                                    formData.append("op", "invia_notifica_aggiornamento_preventivo");
                                    formData.append("e", "contratti_fotovoltaico");
                                    formData.append("id", $("input[name=id]").val());
                                    $.ajax({
                                        type: "POST",
                                        data: formData,
                                        dataType : 'json',			
                                        contentType: false,
                                        processData: false, 
                                        url: ajax_url +  "/fc-ajax.php?",
                                        //async: false,
                                        success: function(json){
                                            if( json.error == 0 ){
                                                $("#overlay").hide();
                                                M.toast({html: json.msg});
                                            }
                                            else{
                                                $("#overlay").hide();
                                                $("#invia-notifica-aggiornamento-preventivo").css("visibility", "visible");
                                                fcalert(json.msg);
                                            }
                                        }
                                    });
                                    
                                    return false;
                                    });
                            </script>
