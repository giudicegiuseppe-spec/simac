<script id="lockTopResidenza">(function(){
  var el=document.getElementById('top_residenza');
  if(!el) return;
  var last=el.value||'';
  function save(){ last=el.value; }
  function ensure(){ if(el.value!==last && document.activeElement===el){ /* do not override while typing */ last=el.value; return; } if(el.value!==last){ el.value=last; } }
  el.addEventListener('input', save, true);
  el.addEventListener('change', save, true);
  // Guard timer to restore any unwanted external changes
  setInterval(ensure, 200);
})();</script><script id="lockTopInstallazione">(function(){
  var el=document.getElementById('top_installazione');
  if(!el) return;
  var last=el.value||'';
  function save(){ last=el.value; }
  function ensure(){ if(el.value!==last && document.activeElement===el){ last=el.value; return; } if(el.value!==last){ el.value=last; } }
  el.addEventListener('input', save, true);
  el.addEventListener('change', save, true);
  setInterval(ensure, 200);
})();</script><script id="removeNotesEverywhere">(function(){
  function removeNode(n){ if(n && n.parentNode){ n.parentNode.removeChild(n); } }
  function sweep(root){
    root = root || document;
    // remove section.note blocks
    root.querySelectorAll('section.note').forEach(removeNode);
    // remove h2 'Note Studio Tecnico' and its parent section if any
    root.querySelectorAll('h2').forEach(function(h){
      if((h.textContent||'').trim().toLowerCase()==='note studio tecnico'){
        var sec = h.closest('section');
        removeNode(sec || h);
      }
    });
    // remove common widgets
    ['.note-wrapper', '.nota-reply', '.add_nota', '#add_nota_wrap'].forEach(function(sel){
      root.querySelectorAll(sel).forEach(removeNode);
    });
  }
  function wire(){ sweep(document); }
  if(document.readyState==='loading') document.addEventListener('DOMContentLoaded', wire); else wire();
  try{
    var obs = new MutationObserver(function(){ sweep(document); });
    obs.observe(document.body, {childList:true, subtree:true});
  }catch(e){}
})();
</script></body>
</html>